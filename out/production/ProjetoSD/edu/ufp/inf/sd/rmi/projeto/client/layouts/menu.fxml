<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="1080.0" prefWidth="1820.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ufp.inf.sd.rmi.projeto.client.MenuController">
   <children>
      <MenuBar prefHeight="42.0" prefWidth="1831.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleReadFile" text="Open Text File" />
                  <MenuItem mnemonicParsing="false" onAction="#handleReadBinFile" text="Open Bin File" />
                  <MenuItem mnemonicParsing="false" onAction="#handleSaveFile" text="Save to Text File" />
                  <MenuItem mnemonicParsing="false" onAction="#handleSaveBinFile" text="Save to Bin File" />
                  <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleAbout" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <TabPane layoutX="-1.0" layoutY="42.0" prefHeight="1048.0" prefWidth="1832.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab fx:id="createTaskTab" text="Create Task">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="nameTaskTF" layoutX="841.0" layoutY="143.0" />
                        <ComboBox fx:id="hashTypeCB" layoutX="841.0" layoutY="183.0" prefHeight="26.0" prefWidth="149.0" />
                        <Button fx:id="createTaskBut" layoutX="876.0" layoutY="279.0" mnemonicParsing="false" onAction="#handlerCreateTask" text="Create Task" />
                        <Label layoutX="694.0" layoutY="146.0" prefHeight="18.0" prefWidth="82.0" text="Name" />
                        <Label layoutX="694.0" layoutY="187.0" prefHeight="18.0" prefWidth="86.0" text="Hash type" />
                        <Label layoutX="694.0" layoutY="227.0" prefHeight="18.0" prefWidth="86.0" text="Hash pass" />
                        <Label fx:id="messageCreateTask" layoutX="768.0" layoutY="327.0" prefHeight="67.0" prefWidth="295.0" />
                        <TextArea fx:id="hashPassTA" layoutX="816.0" layoutY="227.0" prefHeight="17.0" prefWidth="200.0" />
                     </children></AnchorPane>
               </content></Tab>
            <Tab fx:id="listTasksTab" onSelectionChanged="#handlerListTasks" text="List Tasks">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="1018.0" prefWidth="1454.0">
                     <children>
                    <AnchorPane layoutX="23.0" layoutY="49.0" minHeight="0.0" minWidth="0.0" prefHeight="0.0" prefWidth="1820.0" />
                        <TableView fx:id="tasksTable" layoutX="23.0" layoutY="33.0" prefHeight="682.0" prefWidth="735.0">
                           <columns>
                              <TableColumn fx:id="nameCol" prefWidth="145.2000305056572" text="Name" />
                              <TableColumn fx:id="hashTypeCol" prefWidth="143.20001220703125" text="Hash Type" />
                              <TableColumn fx:id="creditsPerWordCol" prefWidth="148.199951171875" text="Credits per Word" />
                              <TableColumn fx:id="creditsTotalCol" prefWidth="148.199951171875" text="Credits Total" />
                              <TableColumn fx:id="availableCol" prefWidth="148.199951171875" text="Available" />
                           </columns>
                        </TableView>
                        <Pagination fx:id="listTasksPagination" layoutX="318.0" layoutY="715.0" prefHeight="60.0" prefWidth="163.0" />
                        <Label fx:id="messageJoinTask" layoutX="817.0" layoutY="48.0" prefHeight="69.0" prefWidth="323.0" />
                        <Label fx:id="nameTaskSelectedLabel" layoutX="862.0" layoutY="134.0" prefHeight="18.0" prefWidth="225.0" />
                        <Button fx:id="jointTaskBut" layoutX="951.0" layoutY="170.0" mnemonicParsing="false" onAction="#handlerJoinTask" prefHeight="20.0" prefWidth="48.0" text="Join" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
